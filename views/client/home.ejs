<!doctype html>
<html>
<% include ../partial/head.ejs %>
<body class="">
  <header>
<% include ../partial/heady.ejs %>
<% include ../partial/search-list.ejs %>
</header>
<!-- ASIDE NAV AND CONTENT -->
<div class="line">
   <div class="box">
      <div class="margin2x">
         <!-- CONTENT -->
         <section class="s-12 m-10 l-10 right">
          <div class="s-12 m-9 l-9 ">
           <div class="s-12 m-6 l-6 homewelcome">
             <h3>Welcome To List Business.</h3>
             <h5>Starting A Business? Growing A Business? List Business Is Here To Help You.</h5>

            </div>
             <div class="s-12 m-6 l-6 homewelcome" id="right">
               <h3>Welcome To List Business.</h3>
               <h5>Starting A Business? Growing A Business? List Business Is Here To Help You.</h5>

              </div>

            <h1>Top Business Listing</h1>
            <div class="margin ">

              <% for(i=0; i<accounts.length; i++) { %>
               <div class="s-12 m-6 l-6">
                 <a href="/clist<%= accounts[i].id %>"><h3 class="" style="color : blue"><%= accounts[i].title %></h3></a>
                 <div class="s-12 m-4 l-4">

                  <a href="/clist<%= accounts[i].id %>"><img src="/<%= accounts[i].logo %>" class="img-responsive thumbnail" style="height:117px; width:100px;"></a>
                  </div>
                  <div class="s-12 m-8 l-8" style="padding-left : 4px;">
                  <h4 class="margin-bottom"><%= accounts[i].location %></h4>
                  <h4 class="margin-bottom" style=" color : #4d4d00;"><strong> <%= accounts[i].description %></strong></h4>
               </div>
               <div class="s-12 m-8 l-8">
                 <i class="fa fa-phone"><%= accounts[i].phoneno %> |</i>
                 <a href="<%= accounts[i].website %> " target="_blank"><i class="fa fa-globe"></i>Visit</a>
               </div>
               </div>
                <% } %>

            </div>
          </div>
          <div class="s-12 m-3 l-3 packages">
            <div class="subtitle" >List Packages.</div>
            <div class="s-12 l-12 m-12 plans platnum">
            <div class="subtitle" style="text-align:center">Platnum Plan</div>
            <ul>
            <li>Upto 30 Products </li>
            <li>Always Among The Top On the search list.</li>
            <li>Monthly Subscribtion. </li>
            </ul>
            <h4 style="text-align:center">For Only $.50</h4>
            <button class="btn-default center" >Subscribe</button>
          </div>
          <div class="s-12 l-12 m-12 plans gold">
          <div class="subtitle" style="text-align:center">Gold Plan</div>
          <ul>
          <li>Upto 15 Products </li>
          <li>Always Among The Top On the search list.</li>
          <li>Monthly Subscribtion. </li>
          </ul>
          <h4 style="text-align:center">For Only $.35</h4>
          <button class="btn-default center" >Subscribe</button>
        </div>
        <div class="s-12 l-12 m-12 plans basic">
        <div class="subtitle" style="text-align:center">Basic Plan</div>
        <ul>
        <li>Upto 6 Products </li>
        <li>Always Among The Top On the search list.</li>
        <li>Monthly Subscribtion. </li>
        </ul>
        <h4 style="text-align:center">For Only $.20</h4>
        <button class="btn-default center" >Subscribe</button>
      </div>
          </div>
         </section>
         <!-- ASIDE NAV -->
             <% include ../partial/aside.ejs %>

      </div>
   </div>
</div>
<!-- FOOTER -->
<% include ../partial/footer.ejs %>

<% include ../partial/footer-script.ejs %>
</body>
</html>
